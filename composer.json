{
    "name": "creatiom/sulu-cookie-consent-bundle",
    "type": "library",
    "description": "Symfony bundle for implementing Cookie Consent to comply to AVG/GDPR.",
    "version": "1.2",
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.6",
        "doctrine/doctrine-bundle": "^1.10 || ^2.0",
        "doctrine/orm": "^2.5",
        "symfony/asset": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/doctrine-bridge": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/templating": "^6.4",
        "symfony/translation": "^7.0",
        "symfony/twig-bridge": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "twig/twig": "^2.6 || ^3.0",
        "sulu/web-twig": "^2.0 || ^3.0",
        "sulu/form-bundle": "^2.0 || ^3.0",
        "creatiom/sulu-block-bundle": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^7.5 || ^9.5",
        "symfony/yaml": "^3.4 || ^4.2 || ^5.0 || ^6.0"
    },
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "Creatiom\\SuluCookieConsentBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creatiom\\SuluCookieConsentBundle\\Tests\\": "/Tests"
        }
    },
    "config": {
         "sort-packages": true
    }
}